# Community Plugins Configuration
COMMUNITY_PLUGINS_ORG=roadrunner-plugins

# GitHub Configuration
GITHUB_TOKEN=your_github_personal_access_token_here

# Velox Discovery Configuration
VELOX_DISCOVERY_ENABLED=true
VELOX_GITHUB_ORG=roadrunner-plugins
VELOX_MANIFEST_FILE=.plugin.yaml

# Webhook Configuration
VELOX_WEBHOOK_SECRET=your_secure_webhook_secret_here

# RoadRunner Version
RR_VERSION=v2025.1.1

# Velox Builder Configuration
VELOX_BINARY_PATH=vx
VELOX_BUILD_TIMEOUT=300
VELOX_TEMP_DIR=/tmp/velox-builds
